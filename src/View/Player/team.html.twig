{% extends 'layout.html.twig' %}

{% block content %}
<h1>Team</h1>
<ul>
    {% for player in players %}
    <li><a href="/team/{{ player.id }}">{{ player.firstname }}. {{ player.lastname }}</a></li>
    {% else %}
    <li>Nothing to display</li>
    {% endfor %}
</ul>
<a href="/item/add">Ajouter</a>
{% endblock %}
